{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/algogo/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/algogo/tsconfig.app.json",
    "watchAssets": true
  },
  "monorepo": true,
  "root": "apps/algogo",
  "projects": {
    "algogo": {
      "type": "application",
      "root": "apps/algogo",
      "entryFile": "main",
      "sourceRoot": "apps/algogo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/algogo/tsconfig.app.json",
        "assets": [
          {
            "include": "../src/config/env/*.env",
            "outDir": "./dist/apps/algogo"
          }
        ],
        "watchAssets": true
      }
    },
    "crawler": {
      "type": "application",
      "root": "apps/crawler",
      "entryFile": "main",
      "sourceRoot": "apps/crawler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/crawler/tsconfig.app.json"
      }
    },
    "compiler": {
      "type": "application",
      "root": "apps/compiler",
      "entryFile": "main",
      "sourceRoot": "apps/compiler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/compiler/tsconfig.app.json"
      }
    }
  }
}