{
  "name": "algogo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild:algogo": "rm -rf ./dist/apps/algogo",
    "prebuild:crawler": "rm -rf ./dist/apps/crawler",
    "prebuild:compiler": "rm -rf ./dist/apps/compiler",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "dev": "NODE_ENV=development nest start --watch",
    "build:algogo": "pnpm prebuild:algogo && nest build algogo",
    "dev:algogo": "pnpm prebuild:algogo && NODE_ENV=development nest start algogo --watch",
    "dev:compiler": "pnpm prebuild:compiler && NODE_ENV=development nest start compiler --watch",
    "dev:crawler": "pnpm prebuild:crawler && NODE_ENV=development nest start crawler --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/algogo/main",
    "prisma:generate": "npx prisma generate --schema=./libs/shared-prisma/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./libs/shared-prisma/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=./libs/shared-prisma/prisma/schema.prisma",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/algogo/test/jest-e2e.json",
    "test:crawler:e2e": "jest --config ./apps/crawler/test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "@keyv/redis": "^4.5.0",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bullmq": "^10.2.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.4.19",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.19",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.19",
    "@nestjs/platform-socket.io": "^10.4.19",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/websockets": "^10.4.19",
    "@prisma/client": "^5.22.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.56.1",
    "cache-manager": "^7.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "keyv": "^5.3.4",
    "multer": "1.4.5-lts.1",
    "nest-winston": "^1.10.2",
    "node-html-parser": "^6.1.13",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "pidusage": "^3.0.2",
    "prisma": "^5.22.0",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.2",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "uuidv7": "^1.0.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.19",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.4",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "prisma-markdown": "^1.0.9",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}