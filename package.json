{
  "name": "algogo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild:algogo": "rm -rf ./dist/apps/algogo",
    "prebuild:crawler": "rm -rf ./dist/apps/crawler",
    "prebuild:compiler": "rm -rf ./dist/apps/compiler",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "dev": "NODE_ENV=development nest start --watch",
    "build:algogo": "pnpm prebuild:algogo && nest build algogo",
    "dev:algogo": "pnpm prebuild:algogo && NODE_ENV=development nest start algogo --watch",
    "dev:compiler": "pnpm prebuild:compiler && NODE_ENV=development nest start compiler --watch",
    "dev:crawler": "pnpm prebuild:crawler && NODE_ENV=development nest start crawler --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/algogo/main",
    "prisma:generate": "npx prisma generate --schema=./libs/shared-prisma/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./libs/shared-prisma/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=./libs/shared-prisma/prisma/schema.prisma",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/algogo/test/jest-e2e.json",
    "test:crawler:e2e": "jest --config ./apps/crawler/test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.623.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bullmq": "^10.2.0",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@prisma/client": "^5.17.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.2.0",
    "fs": "0.0.1-security",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nest-winston": "^1.10.0",
    "node-html-parser": "^6.1.13",
    "passport-google-oauth20": "^2.0.0",
    "passport-oauth2": "^1.8.0",
    "pidusage": "^3.0.2",
    "prisma": "^5.17.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.4",
    "uuidv7": "^1.0.1",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.14",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios": "link:@types/@nestjs/axios",
    "config": "link:@types/@nestjs/config",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "passport": "link:@types/@nestjs/passport",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/(.*)$": "<rootDir>/libs/$1",
      "^@app/exception-filter(|/.*)$": "<rootDir>/libs/exception-filter/src/$1",
      "^@apps/lib/mobules/exception-filter(|/.*)$": "<rootDir>/libs/exception-filter/src/$1",
      "^@app/filter(|/.*)$": "<rootDir>/libs/filter/src/$1",
      "^@app/module(|/.*)$": "<rootDir>/libs/module/src/$1",
      "^@app/interceptor(|/.*)$": "<rootDir>/libs/interceptor/src/$1"
    }
  }
}